{
  "devCommand": "uvicorn main:app --host 0.0.0.0 --port 3000",
  "builds": [
    {
      "src": "titiler/xarray/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "titiler/xarray/main.py"
    }
  ],
  "env": {
    "APP_MODULE": "titiler.xarray.main:app"
  }
}
